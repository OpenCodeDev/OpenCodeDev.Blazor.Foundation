            </OrbitSlides>
            <OrbitBullets>
                <OrbitBullet Index="0" IsActive="true" />
                <OrbitBullet Index="1" />
            </OrbitBullets>
        </OrbitWrapper>

        <hr />
    </SectionLeft>
