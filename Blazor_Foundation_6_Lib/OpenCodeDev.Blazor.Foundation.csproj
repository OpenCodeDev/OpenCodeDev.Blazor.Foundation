<Project Sdk="Microsoft.NET.Sdk.Razor">  <PropertyGroup>	<TargetFrameworks>net6.0</TargetFrameworks>	<Authors>OpenCodeDev</Authors>	<Company>OpenCodeDev</Company>	<Product>Blazor Foundation</Product>	<Description>Foundation 6 components converted as blazor component mixing c# and javascript to control dynamic components</Description>	<Copyright>OpenCodeDev © 2023</Copyright>	<PackageLicenseExpression></PackageLicenseExpression>	<PackageProjectUrl>https://bf.opencodedev.com/</PackageProjectUrl>	<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>	<IsPackable>true</IsPackable>	<ApplicationIcon />	<OutputType>Library</OutputType>	<StartupObject />	<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>	<NeutralLanguage>en</NeutralLanguage>	<PackageReleaseNotes>See https://github.com/OpenCodeDev/OpenCodeDev.Blazor.Foundation</PackageReleaseNotes>	<PackageId>OpenCodeDev.Blazor.Foundation</PackageId>	<AssemblyVersion>3.1.0.8</AssemblyVersion>	<Version>3.1.0.8</Version>	<FileVersion>3.1.0.8</FileVersion>	<PackageIcon>icon_3d.1.png</PackageIcon>	<PackageIconUrl />	<RepositoryUrl>https://github.com/OpenCodeDev/OpenCodeDev.Blazor.Foundation</RepositoryUrl>	<RepositoryType>Open Source</RepositoryType>	<PackageTags>zurb-foundation, blazor, c#, RESPONSIVE, mobile-first, blazor-foundation, .net6</PackageTags>	<AssemblyName>OpenCodeDev.Blazor.Foundation</AssemblyName>	<RootNamespace>OpenCodeDev.Blazor.Foundation</RootNamespace>	<Title>Blazor Foundation</Title>	<PackageReadmeFile>README.md</PackageReadmeFile>	<PackageOutputPath>$(OutputPath)</PackageOutputPath>	<PackAsTool>False</PackAsTool>	<GenerateDocumentationFile>True</GenerateDocumentationFile>	<IncludeSymbols>False</IncludeSymbols>	<SymbolPackageFormat>snupkg</SymbolPackageFormat>	<EnforceCodeStyleInBuild>False</EnforceCodeStyleInBuild>  </PropertyGroup>  <ItemGroup>	<Content Remove="compilerconfig.json" />  </ItemGroup>  <ItemGroup>	<_ContentIncludedByDefault Remove="compilerconfig.json" />	<_ContentIncludedByDefault Remove="wwwroot\css\bf6_complete.min.css" />  </ItemGroup>  <ItemGroup>	<None Include="..\icon_3d.1.png">	  <Pack>True</Pack>	  <PackagePath></PackagePath>	</None>	<None Include="..\LICENSE.txt">	  <Pack>True</Pack>	  <PackagePath></PackagePath>	</None>	<None Include="..\README.md">
  <Pack>True</Pack>
  <PackagePath>\</PackagePath>
</None>  </ItemGroup>  <ItemGroup>	<Folder Include="wwwroot\img\" />	<Folder Include="_SCSS\Blazor_Foundation\Components\Containers\" />  </ItemGroup>  <ItemGroup Condition=" '$(TargetFramework)' == 'net6.0'">	<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="6.0.0" allowedVersions="[6,7)" />	<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="6.0.0" allowedVersions="[6,7)" />	<PackageReference Include="Newtonsoft.Json" Version="13.*" allowedVersions="[13,14)" />
</ItemGroup>	<!-- Compile SCSS files into CSS -->	<Target Name="CompileGlobalSass" BeforeTargets="Compile">		<Message Text="Compiling CSSsss" Importance="high" />		<!-- foundation.js  -->			<Exec Command="node-sass $(ProjectDir)_SCSS\bf6_only.scss $(ProjectDir)wwwroot\css\bf6_only.css" />		<Exec Command="node-sass $(ProjectDir)_SCSS\bf6_prototype.scss $(ProjectDir)wwwroot\css\bf6_prototype.css" />		<Exec Command="node-sass $(ProjectDir)_SCSS\bf6_rtl.scss $(ProjectDir)wwwroot\css\bf6_rtl.css" />		<Exec Command="node-sass $(ProjectDir)_SCSS\bf6_standard.scss $(ProjectDir)wwwroot\css\bf6_standard.css" />	</Target>			<!-- Compile SCSS files into CSS -->	<Target Name="MergeJS" BeforeTargets="Compile">		<Message Text="Merging Javascripts Files" Importance="high" />		<!-- Copy Individual Files -->		<Exec Command="powershell.exe –NonInteractive –ExecutionPolicy Unrestricted -Command &quot;&amp; { Get-Content &quot;$(ProjectDir)_JS\src\foundation.min.js&quot; | Set-Content &quot;$(ProjectDir)wwwroot\js\foundation.js&quot;} &quot;" />		<Exec Command="powershell.exe –NonInteractive –ExecutionPolicy Unrestricted -Command &quot;&amp; { Get-Content &quot;$(ProjectDir)_JS\src\jquery.js&quot; | Set-Content &quot;$(ProjectDir)wwwroot\js\jquery.js&quot;} &quot;" />		<Exec Command="powershell.exe –NonInteractive –ExecutionPolicy Unrestricted -Command &quot;&amp; { Get-Content &quot;$(ProjectDir)_JS\src\what-input.js&quot; | Set-Content &quot;$(ProjectDir)wwwroot\js\what-input.js&quot;} &quot;" />		<!-- blazor-foundation.js legacy support -->		<Exec Command="powershell.exe –NonInteractive –ExecutionPolicy Unrestricted -Command &quot;&amp; { Get-Content &quot;$(ProjectDir)_JS\src\jquery.js&quot;, &quot;$(ProjectDir)_JS\src\newline.js&quot;, &quot;$(ProjectDir)_JS\src\what-input.js&quot;, &quot;$(ProjectDir)_JS\src\newline.js&quot;, &quot;$(ProjectDir)_JS\src\blazor-foundation.js&quot; | Set-Content &quot;$(ProjectDir)wwwroot\js\bf6_only.js&quot;} &quot;" />		<!-- bf6_standard.js -->		<Exec Command="powershell.exe –NonInteractive –ExecutionPolicy Unrestricted -Command &quot;&amp; { Get-Content &quot;$(ProjectDir)_JS\src\jquery.js&quot;, &quot;$(ProjectDir)_JS\src\newline.js&quot;, &quot;$(ProjectDir)_JS\src\what-input.js&quot;, &quot;$(ProjectDir)_JS\src\newline.js&quot;, &quot;$(ProjectDir)_JS\src\foundation.min.js&quot;, &quot;$(ProjectDir)_JS\src\newline.js&quot;, &quot;$(ProjectDir)_JS\src\blazor-foundation.js&quot; | Set-Content &quot;$(ProjectDir)wwwroot\js\bf6_standard.js&quot;} &quot;" />		<!-- bf6_only.js -->		<Exec Command="powershell.exe –NonInteractive –ExecutionPolicy Unrestricted -Command &quot;&amp; { Get-Content &quot;$(ProjectDir)_JS\src\jquery.js&quot;, &quot;$(ProjectDir)_JS\src\newline.js&quot;, &quot;$(ProjectDir)_JS\src\what-input.js&quot;, &quot;$(ProjectDir)_JS\src\newline.js&quot;, &quot;$(ProjectDir)_JS\src\blazor-foundation.js&quot; | Set-Content &quot;$(ProjectDir)wwwroot\js\bf6_only.js&quot;} &quot;" />	</Target></Project>